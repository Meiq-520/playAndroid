<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/blankFragment">

    <fragment
        android:id="@+id/blankFragment"
        android:name="com.mac.book.myplayandroid.BlankFragment"
        android:label="fragment_blank"
        tools:layout="@layout/fragment_blank" >
        <action
            android:id="@+id/action_blankFragment_to_fragmentB"
            app:destination="@id/fragmentB" />
        <action
            android:id="@+id/action_blankFragment_to_fragmentC"
            app:destination="@id/fragmentC" />
        <action
            android:id="@+id/action_blankFragment_to_fragmentD"
            app:destination="@id/fragmentD" />
        <action
            android:id="@+id/action_blankFragment_to_fragmentE"
            app:destination="@id/fragmentE" />
        <action
            android:id="@+id/action_blankFragment_to_fragmentG"
            app:destination="@id/fragmentG" />
    </fragment>
    <fragment
        android:id="@+id/fragmentB"
        android:name="com.mac.book.myplayandroid.FragmentB"
        android:label="fragment_b"
        tools:layout="@layout/fragment_b" />
    <fragment
        android:id="@+id/fragmentC"
        android:name="com.mac.book.myplayandroid.FragmentC"
        android:label="fragment_c"
        tools:layout="@layout/fragment_c" />
    <fragment
        android:id="@+id/fragmentD"
        android:name="com.mac.book.myplayandroid.FragmentD"
        android:label="fragment_d"
        tools:layout="@layout/fragment_d" />
    <fragment
        android:id="@+id/fragmentE"
        android:name="com.mac.book.myplayandroid.FragmentE"
        android:label="fragment_e"
        tools:layout="@layout/fragment_e" />
    <fragment
        android:id="@+id/fragmentG"
        android:name="com.mac.book.myplayandroid.FragmentG"
        android:label="fragment_g"
        tools:layout="@layout/fragment_g" />

</navigation>